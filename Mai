public class TesteInterfce {

    public static void main(String[] args){
        Celular celular = new Celular();

        celular.ligar();

        celular.tocarMusica();
        celular.pararMusica();

        celular.enviarMensagem("Oii tudo bem?","Joana");
        celular.receberMensagem("Olá estou bem e você?", "maria");

        celular.desligar();
    }
}
